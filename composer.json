{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "hanneskod/classtools": "^1.1",
        "league/tactician": "^1.0",
        "myclabs/php-enum": "^1.5",
        "nette/php-generator": "^3.0",
        "nikic/php-parser": "^3.1",
        "roave/better-reflection": "^2.0",
        "symfony/console": "^3.4",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.4",
        "symfony/lts": "v3",
        "symfony/yaml": "^3.4",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "ciaranmcnulty/phpspec-typehintedmethods": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.8",
        "humbug/humbug": "^1.0@RC",
        "mockery/mockery": "^1.0",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "phpspec/phpspec": "^4.2",
        "phpstan/phpstan": "^0.9.1",
        "phpstan/phpstan-phpunit": "^0.9.1",
        "slevomat/coding-standard": "^4.0",
        "squizlabs/php_codesniffer": "^3.1",
        "symfony/dotenv": "^3.4",
        "symfony/phpunit-bridge": "^3.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "MyVendor\\":"lib/src/MyVendor/",
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\MyVendor\\": "lib/spec/MyVendor/",
            "Tests\\MyVendor\\": "lib/tests/MyVendor/",
            "spec\\": "spec/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0DYC285WRNB4ZM4QYV2QCPS",
            "allow-contrib": false
        }
    }
}
